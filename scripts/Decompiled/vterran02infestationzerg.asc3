wait_buildstart 8 drone
build 1 spawning_pool 80
defensebuild_gg 1 zergling
defenseuse_gg 1 zergling
multirun colony_morph
:wait_cc
wait 8

enemyowns_jump command_center cc_done
goto wait_cc

:cc_done
capt_expand
max_workers 14
max_force 303
wait_buildstart 1 spawning_pool
idle_orders 27 128 1 queen 512 command_center 80 health(LessThanPercent,50)|own
under_attack 0
wait 625

creep 4
wait_build 1 spawning_pool
defensebuild_gg 1 infested_terran
capt_expand
train 1 infested_terran
attack_add 3 zergling
attack_add 1 infested_terran
attack_to Loc.13 Loc.12
wait 500

attack_do
attack_clear
farms_timing
build 2 creep_colony 80
train 4 zergling
attack_add 4 zergling
attack_prepare
wait 500

attack_do
attack_clear
wait 850

train 6 zergling
attack_add 6 zergling
attack_to Loc.13 Loc.12
wait 500

attack_do
attack_clear
build 3 creep_colony 80
wait_buildstart 3 creep_colony
build 2 sunken_colony 80
defenseuse_gg 1 infested_terran
wait 1355

build 1 evolution_chamber 60
train 12 zergling
attack_add 12 zergling
attack_add 1 infested_terran
attack_prepare
wait 500

attack_do
attack_clear
upgrade 1 z_carapace 30
define_max 2 infested_terran
train 6 zergling
attack_add 6 zergling
attack_to Loc.13 Loc.12
wait 500

attack_do
attack_clear
:loop
wait 1500

train 14 zergling
attack_add 14 zergling
attack_prepare
wait 500

attack_do
attack_clear
wait 1850

train 2 infested_terran
attack_add 2 infested_terran
attack_prepare
wait 500

attack_do
attack_clear
goto loop

:colony_morph
wait_build 1 spawning_pool
wait 8

bring_jump 14 Exactly 0 group_men Loc.16 colony_morph
issue_order 43 1 creep_colony Loc.16 Loc.16 sunken_colony 0
stop

