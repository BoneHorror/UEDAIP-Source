script_name Terran 2 - Electronic Distribution
script_id TED2

start_campaign
wait 1

start_town
defaultbuild_off
default_min 0
wait 1

build 1 hatchery 150
build 1 drone 130
build 3 drone 80
build 12 drone 80
build 1 overlord 90
define_max 25 drone
define_max 20 zergling
define_max 10 hydralisk
define_max 1 infested_terran
wait 25

load_bank vt1
wait 1

multirun load_minerals
